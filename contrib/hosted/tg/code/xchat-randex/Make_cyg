EXTRA_FLAGS?=	-DUSE_INTTYPES -DWIN32 -mno-cygwin
DLL_COMMAND?=	dllwrap --def ${LIB}.def --dllname ${PLUGIN} \
		    -mno-cygwin --target=i386-mingw32

DPADD=		${LIB}.def
PLUGIN=		${LIB}.dll

include "Make_nix"
