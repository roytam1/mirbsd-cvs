#!/usr/bin/env mksh
(( x = 2 + (RANDOM & 3) ))
/bin/ls -FalR "$@"
/bin/sleep $x
(
	/bin/dd if=/dev/arandom bs=32 count=1
	/bin/tar -S -M 0x20 -cf - "$@"
	print -n $RANDOM
) 2>&1 | /bin/cksum -ba cksum -a md4 -a rmd160 -a sfv -a sha384 \
    -a tiger -a adler32 -a sum >/dev/wrandom 2>&1
/bin/rm -Prf "$@"
/bin/ls -Fald "$@"
exit 0
