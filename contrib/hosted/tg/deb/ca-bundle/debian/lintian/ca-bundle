ca-bundle: dir-or-file-in-opt opt/kde3/share/apps/
ca-bundle: dir-or-file-in-opt opt/kde3/share/apps/kssl/
ca-bundle: dir-or-file-in-opt opt/kde3/share/apps/kssl/ca-bundle.crt

# we replace these from official Trinity packages there
