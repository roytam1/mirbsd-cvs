# targetting etch as well
ca-bundle: description-contains-homepage

# we replace these from kdelibs-data-kde3 (PPA) there
ca-bundle: dir-or-file-in-opt opt/kde3/
ca-bundle: dir-or-file-in-opt opt/kde3/share/
ca-bundle: dir-or-file-in-opt opt/kde3/share/apps/
ca-bundle: dir-or-file-in-opt opt/kde3/share/apps/kssl/
ca-bundle: dir-or-file-in-opt opt/kde3/share/apps/kssl/ca-bundle.crt

# we replace these from official Trinity packages there
ca-bundle: dir-or-file-in-opt opt/trinity/
ca-bundle: dir-or-file-in-opt opt/trinity/share/
ca-bundle: dir-or-file-in-opt opt/trinity/share/apps/
ca-bundle: dir-or-file-in-opt opt/trinity/share/apps/kssl/
ca-bundle: dir-or-file-in-opt opt/trinity/share/apps/kssl/ca-bundle.crt

