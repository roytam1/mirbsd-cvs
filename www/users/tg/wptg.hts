#DEPEND users/tg/WPtg.inc
sources=WPtg
sourcedir_WPtg=users/tg

integer notimesort=1
integer parser_usedate=0

#DEPEND mk/parser
. "$TOP"/mk/parser

integer entryp=0
while (( entryp < entries )); do
	ent=${ei_indir[entryp++]}
	if [[ -n ${e_bannercode[ent]} ]]; then
		print -r -- "${e_bannercode[ent]}"
		print
		continue
	elif [[ -n ${e_labbanner[ent]} ]]; then
		title="Lab / ${e_title[ent]}"
		f=http://mirsolutions.de/privat/${e_labbanner[ent]%%,*}.jpg
		cat <<-EOF
			<div class="flbox"><img alt="$title" title="$title"
			 src="$f" style="border:0px;" /></div>

EOF
		[[ ${e_labbanner[ent]} = *,* ]] || continue
		f=${e_labbanner[ent]#*,}
		title="Lab / ${f#*=}"
		f=http://mirsolutions.de/privat/${f%%=*}.jpg
		cat <<-EOF
			<div class="flbox"><img alt="$title" title="$title"
			 src="$f" style="border:0px;" /></div>

EOF
		continue
	fi
	[[ -n ${e_banner[ent]} ]] || continue
	wp=${e_waypoint[ent]%% *}
	ext=${e_banner[ent]}
	if [[ $ext = *' '* ]]; then
		title=${ext#* }
		ext=${ext%% *}
	else
		title=${e_title[ent]}
	fi
	title=${e_waypoint[ent]}│$title
	f=http://mirsolutions.de/privat/$wp.$ext
	[[ $ext = *://* ]] && f=$ext
	cat <<-EOF
		<div class="flbox"><img alt="$title" title="$title"
		 src="$f" style="border:0px;" /></div>

EOF
done | mws_content

