function statpic {
	local alt=$1 pic=$2 link=$3

	mws_content <<-EOF
		<p><a href="$link" style="border:0;"><img
		 style="border:0;" src="$pic" alt="$alt" /></a></p>

EOF
}

function statnopic {
	local alt=$1 link=$2

	mws_content <<-EOF
	<p><span style="border:1px solid black; padding:6px;"><a
	 href="$link">$alt</a></span></p>

EOF
}

