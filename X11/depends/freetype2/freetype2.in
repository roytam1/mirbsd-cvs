prefix=@X11BASE@
exec_prefix=@X11BASE@/bin
libdir=@X11BASE@/lib
includedir=@X11BASE@/include

Name: FreeType 2
Description: A free, high-quality, and portable font engine.
Version: @ft_version@
Requires:
Libs: -Wl,-rpath,${libdir} -L${libdir} -lfreetype -lz
Cflags: -I${includedir}/freetype2 -I${includedir}
