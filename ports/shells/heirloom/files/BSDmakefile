# $MirOS$

PROG=		jsh
CDIAGFLAGS+=	-Wchar-subscripts -Wformat -Wno-format-y2k -Wimplicit \
		-Wmissing-braces -Wsequence-point -Wreturn-type -Wtrigraphs \
		-Wunused-function -Wunused-label -Wunused-variable \
		-Wunused-value -Wuninitialized -Wmultichar -Wpointer-arith
SRCS=		args.c blok.c bltin.c cmd.c ctype.c defs.c echo.c error.c \
		expand.c fault.c func.c getopt.c gmatch.c hash.c hashserv.c \
		io.c jobs.c macro.c main.c mapmalloc.c msg.c name.c print.c \
		pwd.c service.c setbrk.c stak.c string.c strsig.c test.c \
		ulimit.c umask.c version.c word.c xec.c
LINKS=		${BINDIR}/jsh ${TRUEPREFIX}/5bin/sh \
		${BINDIR}/jsh ${TRUEPREFIX}/5bin/jsh

jsh.1: sh.1
	sed '/BEGIN SPELL/,/END SPELL/d' <$> >$@

beforeinstall:
	${BSD_INSTALL_DATA_DIR} ${PREFIX}/5bin

.include <bsd.prog.mk>
