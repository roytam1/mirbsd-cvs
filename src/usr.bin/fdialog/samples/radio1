#!/bin/mksh
# $MirOS: src/usr.bin/fdialog/samples/radio1,v 1.1.7.1 2005/03/06 16:52:42 tg Exp $
# $ekkoBSD: radio1,v 1.4 2004/03/19 18:07:26 dave Exp $
FDIALOG=${FDIALOG=fdialog}

$FDIALOG --radio "Pick a command:" \
	--item "ls /tmp" \
	--item "ls /" \
	--item "id" \
	20 20 \
	--ptitle "Radio sample 1" 
