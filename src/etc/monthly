#!/bin/mksh
# $MirOS: src/etc/monthly,v 1.3 2005/04/29 18:58:14 tg Exp $
# $OpenBSD: monthly,v 1.5 1998/02/02 23:08:57 millert Exp $

export TZ=UTC

if [ -f /etc/monthly.local ];then
	echo ""
	echo "Running monthly.local:"
	. /etc/monthly.local
else
	echo "Nothing to do!"
fi

echo ""
echo "Doing login accounting:"
ac -p | sort -nr +1

echo "."
