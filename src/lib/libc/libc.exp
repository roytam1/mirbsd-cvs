# $MirOS$
#-
# This is a version script for the GNU linker ld(1) to assign
# local visibility to specific symbols that were cross-module
# to facilitate splitting of related artefacts to separate .c
# files.

{
  # Define all symbols to hide
  local:
	a4state;
	arc4random_roundhash;
	arc4random_stir_locked;
	arcfour_byte;
	arcfour_init;
	arcfour_ksa;
	arcfour_ksa256;
};
