echo
echo Booting MirBSD Installer with serial console at COM1:@SPEED@,8N1
echo
set tty com0
stty com0 @SPEED@
echo
echo MirOS BSD #10-current (i386) | (c) 2002-2015 The MirOS Project
echo
echo Booting MirBSD Installer with serial console at COM1:@SPEED@,8N1
echo
boot /bsd
