	test -n "$verbose" && echo "	add to \$CFLAGS $cf_new_cflags" 1>&6

echo "${as_me-configure}:7045: testing add to \$CFLAGS $cf_new_cflags ..." 1>&5


	CFLAGS="$CFLAGS $cf_new_cflags"
