if test "${enable_dired+set}" = set; then
  enableval="$enable_dired"
  test "$enableval" != no && enableval=yes
  if test "$enableval" != "yes" ; then
    use_dired=$enableval 
  else
    use_dired=yes
  fi
else
  enableval=yes 
  use_dired=yes
  
fi

echo "$ac_t""$use_dired" 1>&6

